function ticket(age){
    if(age <=10){
        return 10;

    }
    else if(age <=17){
        return 15;
    }
    else {
        return 20;
    }
}

function bissectile(annee){
    if(annee % 4 == 0 && annee % 100 != 0 || annee % 400 == 0){
        return true;
    }
    else {
        return false;
    }
}



