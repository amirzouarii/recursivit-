function fibonacci(n){
    if(n==0){
        return 0;
    }
    else if(n==1){
        return 1;
    }
    else{
        return fibonacci(n-1) + fibonacci(n-2);
    }
}


function palindrome(ch){
    var ch1 = ch.split("").reverse().join("");
    if(ch1 == ch){
        return true;
    }
    else{
        return false;
    }
}
